<p id="notice"><%= notice %></p>

<h1>Reponses</h1>

<table>
  <thead>
    <tr>
      <th>Lib reponse</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reponses.each do |reponse| %>
      <tr>
        <td><%= reponse.lib_reponse %></td>
        <td><%= link_to 'Show', reponse %></td>
        <td><%= link_to 'Edit', edit_reponse_path(reponse) %></td>
        <td><%= link_to 'Destroy', reponse, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reponse', new_reponse_path %>
